//  HOW TO CREATE AN AVERAGE FROM AN ARRAY USING .reduce()

let array1 = [2,3,4,8,10,12]

 let averageScore = (array1.reduce((accumulator, currentValue) => accumulator + currentValue, 0)) / array1.length;
    return averageScore;
  }
  
  
